// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  IDUSUARIO Int    @id @default(autoincrement())
  USUARIO   String
  SENHA     String
  EMAIL     String @unique
  CPF       String @unique
  ATIVO     String @default("S")
}
